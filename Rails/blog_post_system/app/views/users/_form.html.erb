<%= form_with scope: :user, url: users_path, local: true do |form| %>
  <p>
  	<%= form.label :Name %><br>
  	<%= form.text_field :name %>
  </p>

  <p>
  	<%= form.label :Contact %><br>
  	<%= form.text_field :contact%>
  </p>

  <p>
  	<%= form.label :Payment %><br>
  	<%= form.text_field :paid%>
  </p>

  <p>
  	<%= form.label :Address %><br>
  	<%= form.text_field :address%>
  </p>

  <p>
  	<%= form.submit :Submit %>
  </p>
<% end %>