<table>
  <thead>
    <tr>
    	<th><%= sortable "name", "Name" %></th>
      <th><%= sortable "imageable_type", "Imageable_type" %></th>
      <th><%= sortable "imageable_id", "Imageable_id" %></th>
    </tr>
  </thead>
  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= picture.name %></td>
        <td><%= picture.imageable_id %></td>
        <td><%= picture.imageable_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div>
  <%= will_paginate %>
</div>
