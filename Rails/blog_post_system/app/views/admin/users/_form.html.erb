
    
<h4><b>User Details</b></h4><br>
<% if user.errors.any? %>
<div id="error_explanation">
  <h4><%= pluralize(user.errors.count, "error") %> Prohibited this user from benging saved:</h4>
  <ul>
    <% user.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
</div>
<% end %>
