<br><h4 style="text-align: center"><b> Listing Users </b></h4><br><br>
<table>
  <thead>
    <tr>
    	<th> First Name </th>
      <th> Last Name </th>
      <th> Email </th>
      <th> Contact </th>
      <th> Payment </th>
      <th> Address </th>
      <th></th>
    </tr>
  </thead>
  <tbody id="users-list">
    <%= render partial: 'users'%>
  </tbody>
</table><br><br>

<div style="text-align: center">
  <%= link_to "Previous Page", admin_users_path(page: @page - 1) %>
  |
  <%= link_to "1", admin_users_path(page: 1) %>
  |
  <%= link_to "2", admin_users_path(page: 2) %>
  |
  <%= link_to "3", admin_users_path(page: 3) %>
  |
  <%= "..." %>
  |
  <%= link_to "Next Page", admin_users_path(page: @page + 1) %>
</div>

<div class="link_style">
  <%= link_to 'New User', new_admin_user_path %><br>
</div>

<style type="text/css">
  .link_style {
  	background-color: #ccffff;
  	color: white;
  	padding: 14px 25px;
  	text-align: center;
  	text-decoration: none;
  	display: inline-block;
  }

  a:hover, a:active {
  	background-color: white;
  }
</style>